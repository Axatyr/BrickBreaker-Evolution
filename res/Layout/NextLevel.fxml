<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<SplitPane fx:id="mainPanel" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.scene.ControllerNextLevel">
   <items>
      <VBox alignment="TOP_CENTER" spacing="10.0" style="-fx-background-image: url(&quot;/Images/mainMenuImg/background.png&quot;);">
         <children>
            <Label fx:id="lblLevel" text="LEVEL" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="60.0" />
               </font>
            </Label>
            <Label text="COMPLETE!" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="50.0" />
               </font>
            </Label>
            <Label fx:id="lblLives" text="YOUR LIVES: " textFill="WHITE">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label fx:id="lblScore" text="YOUR SCORE: " textFill="WHITE">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Button fx:id="btnNext" mnemonicParsing="false" onMouseClicked="#goToNextLevel" text="NEXT LEVEL">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font></Button>
            <Button fx:id="btnMenu" mnemonicParsing="false" onMouseClicked="#backToMenu" text="MENU">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font></Button>
         </children>
      </VBox>
   </items>
</SplitPane>
